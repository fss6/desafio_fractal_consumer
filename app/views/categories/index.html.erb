<div class="box box-info">
  <div class="box-header with-border">
    <h3 class="box-title">
      <strong> <%= t(:other, scope: [:models, :category]) %> </strong>
    </h3>
  </div>
  <div class="box-body">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th><%= t(:name, scope: [:models, :category, :attributes]) %></th>
        </tr>
      </thead>
      <tbody>
        <% get_collection(@categories).each do |category| %>
          <tr>
            <td>
              <%= get_attribute_value_from(category, :name) %>
            </td>
          </tr>
          <% end %>
      </tbody>
    </table>
  </div>
</div>